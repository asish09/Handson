package reverse;
class Student{

	int rollno;
	String name;
	int marks;
	
}
public class Demo2 {

	public static void main(String[] args) {
		Student s1=new Student();
		s1.rollno=1;
		s1.name="asish";
		s1.marks=69;
		
		Student s2=new Student();
		s2.rollno=2;
		s2.name="salman";
		s2.marks=79;
		
		Student s3=new Student();
		s3.rollno=3;
		s3.name="sharukh";
		s3.marks=90;
		
		Student stu[]=new Student[3];
		stu[0]=s1;
		stu[1]=s2;
		stu[2]=s3;
		
		for(int i=0;i<stu.length;i++)
		{
			System.out.println(stu[i].name+" "+stu[i].marks+" "+stu[i].rollno);
		}

	}

}
